@model ShoeEcommerce.Model.AdminManager.RegisterNotify

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Share/LayoutAdmin.cshtml";

}

<div class="panel panel-body">
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="id_Acc" class="control-label"></label>
                    <input asp-for="id_Acc" class="form-control" />
                    <span asp-validation-for="id_Acc" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="id_Mer" class="control-label"></label>
                    <input asp-for="id_Mer" class="form-control" />
                    <span asp-validation-for="id_Mer" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="createdDate" class="control-label"></label>
                    <input asp-for="createdDate" class="form-control" />
                    <span asp-validation-for="createdDate" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="checkbox">
                        <label>
                            <input asp-for="Checked" /> @Html.DisplayNameFor(model => model.Checked)
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="checkbox">
                        <label>
                            <input asp-for="stt" /> @Html.DisplayNameFor(model => model.stt)
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <input type="submit" value="Tạo" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>

    <div>
        <a asp-action="Index">Về danh sách</a>
    </div>
</div>


